{
    "name": "nbcx/nLogin",
    "description":"Third party login collection",
    "keywords":["oauth","oauth2","login"],
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Collin",
            "email": "collin@nb.cx",
            "homepage":"http://nb.cx"
        }
    ],
    "homepage": "http://github.com/nbcx/nLogin",
    "autoload": {
        "psr-0": { "nLogin": "src/" }
    },
    "require":{
        "php":">=5.4.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "aws/aws-sdk-php": "~2.8",
        "firebase/php-jwt": "~2.2",
        "predis/predis": "dev-master",
        "thobbs/phpcassa": "dev-master",
        "mongodb/mongodb": "^1.1"
    },
    "suggest": {
        "predis/predis": "Required to use Redis storage",
        "thobbs/phpcassa": "Required to use Cassandra storage",
        "aws/aws-sdk-php": "~2.8 is required to use DynamoDB storage",
        "firebase/php-jwt": "~2.2 is required to use JWT features",
        "mongodb/mongodb": "^1.1 is required to use MongoDB storage"
    }
}
